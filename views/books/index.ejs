<h2>Search Books</h2>
<form action="/books" method="GET">
  <div>
    <label for="title">Title</label>
    <input
      type="text"
      name="title"
      id="title"
      value="<%= searchOption.title %>"
    />
  </div>
  <div>
    <label for="publishedAfter">Published After</label>
    <input
      type="date"
      name="publishedAfter"
      id="publishedAfter"
      value="<%= searchOption.publishedAfter %>"
    />
  </div>
  <div>
    <label for="publishedBefore">Published Before</label>
    <input
      type="date"
      name="publishedBefore"
      id="publishedBefore"
      value="<%= searchOption.publishedBefore %>"
    />
  </div>

  <button type="submit">Search</button>
</form>
<div>
  <% books.forEach(book => { %>
  <a href="/books/<%= book.id %> ">
    <img height="100" width="100" src="<%= book.coverImagePath %>" alt="" />
  </a>
  <% }) %>
</div>
